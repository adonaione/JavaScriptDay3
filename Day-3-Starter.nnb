                    "items": [
                        {
                            "mime": "application/vnd.code.notebook.stdout",
                            "value": [
                                "Thank you for purchasing the Double Platinum Ticket Plan. It Includes: ",
                                "Championship Game Tickets Included",
                                "Playoff Game Tickets Included",
                                "Parking Pass Every Game",
                                "Ticket to Every Home Game",
                                ""
                            ]
                        }
                    ]
                }
            ]
