                    "items": [
                        {
                            "mime": "application/vnd.code.notebook.stdout",
                            "value": [
                                "{ name: 'Janet', age: 55, city: 'Louiseville' }",
                                ""
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "language": "javascript",
            "source": [
                "// JS does not require quotation around the Keys\n\nlet person2 = {\n    name: \"Max\",\n    age: 33,\n    city: \"Charlotte\"\n}"
            ],
            "outputs": []
